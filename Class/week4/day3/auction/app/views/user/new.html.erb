<div class="row">
  <div class="col-12">
    <h1>Create new user</h1>
    <%= form_for @my_user do |f|%>
    <%= f.label :name%>
    <br>
    <%= f.text_field :name%>
    <br>
    <%= f.label :email%>
    <br>
    <%= f.text_field :email%>
    <br>
    <%= f.button "create"%>
    <% end%>
  </div>
</div>
