<div class="row">
  <div class="col-12">
    <h1>Create new product</h1>
    <%= form_for [@my_user, @my_product] do |f|%>
    <%= f.label :title%>
    <br>
    <%= f.text_field :title%>
    <br>
    <%= f.label :description%>
    <br>
    <%= f.text_area :description%>
    <br>
    <%= f.label :bid_dl%>
    <br>
    <%= f.date_field :bid_dl%>
    <br>
    <%= f.button "create"%>
    <% end%>
  </div>
</div>
