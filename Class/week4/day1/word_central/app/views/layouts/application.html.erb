<!DOCTYPE html>
<html>
  <head>
    <title>WordCentral</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <nav><%= link_to "Text inspections", text_inspections_new_path %> <%= link_to "Asciify my text", asciis_new_path %> <%= link_to "Placeholder text", placeholder_new_path %></nav>
    </div>
    <%= yield %>
  </body>
</html>
